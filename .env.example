# Proxy Configuration (WebShare or similar) - REQUIRED
PROXY_URL=http://username-rotate:password@p.webshare.io:80

# Supabase Configuration
SUPABASE_URL=https://xxx.supabase.co
SUPABASE_ANON_KEY=xxx
SUPABASE_SERVICE_KEY=xxx
# Direct Postgres connection (for migrations via psql)
# Find this in Supabase Dashboard → Settings → Database → Connection string
SUPABASE_DB_URL=postgres://postgres:[PASSWORD]@db.xxx.supabase.co:5432/postgres

# Apify Configuration
APIFY_API_KEY=apify_api_xxx

# Scheduler (pick one)
SCRAPE_INTERVAL=6h
# SCRAPE_CRON=0 */6 * * *
SYNC_INTERVAL=180s

# Scraper Settings
SCRAPE_DELAY_MS=500
LOG_LEVEL=info

# Database
DB_PATH=scraper.db
